    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/Bali/PuraKahyanganJagat.owl#Pura_Luhur_Uluwatu">
        <rdf:type rdf:resource="http://www.semanticweb.org/Bali/PuraKahyanganJagat.owl#PuraKahyanganJagat"/>
