    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/Bali/PuraKahyanganJagat.owl#Pura_Gelap">
        <rdf:type rdf:resource="http://www.semanticweb.org/Bali/PuraKahyanganJagat.owl#PuraKahyanganJagat"/>
