        <PuraKahyanganJagat:memilikiPelinggih rdf:resource="http://www.semanticweb.org/Bali/PuraKahyanganJagat.owl#Pelinggih_Segara"/>
        <PuraKahyanganJagat:memilikihariPiodalan rdf:resource="http://www.semanticweb.org/Bali/PuraKahyanganJagat.owl#Anggara_Kasih_Julungwangi"/>
