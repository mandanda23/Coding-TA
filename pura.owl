        <PuraKahyanganJagat:memilikiDesa rdf:resource="http://www.semanticweb.org/Bali/PuraKahyanganJagat.owl#Desa_Perancak"/>
        <PuraKahyanganJagat:memilikiKabupaten rdf:resource="http://www.semanticweb.org/Bali/PuraKahyanganJagat.owl#Jembrana"/>
